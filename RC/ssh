# FIRST GitHub account
Host github.com
	HostName github.com
	User git
	AddKeysToAgent yes
    IdentityFile ~/.ssh/main

# SECOND GitHub account
Host second
    HostName github.com
	User git
	AddKeysToAgent yes
	IdentityFile ~/.ssh/second

# Gitlab account
Host gitlab
    HostName gitedu.alunos.dcc.fc.up.pt
    User git
    AddKeysToAgent yes
    IdentityFile ~/.ssh/gitlab
